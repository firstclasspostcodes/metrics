{
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@google/semantic-release-replace-plugin",
      {
        "replacements": [
          {
            "files": ["package.json"],
            "from": "\"version\": \"[\\.0-9]\"",
            "to": "\"version\": \"${nextRelease.gitTag}\"",
            "results": [
              {
                "file": "package.json",
                "hasChanged": true,
                "numMatches": 1,
                "numReplacements": 1
              }
            ],
            "countMatches": true
          }
        ]
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["package.json"],
        "message": "chore: Updated package.json version ${nextRelease.version} [skip ci]"
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/github"
  ]
}